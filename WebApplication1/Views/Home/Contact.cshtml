                                            <li><a href="#">Супы</a></li>
                                            <li><a href="#">Соусы</a></li>
                                            <li><a href="#">Напитки</a></li>
                                        </ul>
                                    </li>
                                    <li><a href="#">Заказ</a></li>
                                    <li><a href="@Url.Action("Contact", "Home")">Контакты</a></li>
                                </ul>
                                <form class="navbar-form navbar-left" role="search">
                                    <div class="form-group">
                                        <input type="text" class="form-control" placeholder="Search">
                                    </div>
                                </form>
                            </div><!-- /.navbar-collapse -->

                            <a class="navbar-brand" href="@Url.Action("Index", "Home")">
                                <div class="logo hidden-md hidden-sm hidden-xs"></div>
                                <div class="logobg"></div>
                            </a>

                            <div class="basketbg"></div>
                            <a href="#">
                                <div class="basket hidden-md hidden-sm hidden-xs basket-style">
                                    <img src="../../Content/Images/Controls/basket-icon.png" />
                                    <span>Корзина</span>
                                </div>
                            </a>

                        </div><!-- /.container-fluid -->
                    </nav>
                </div>
            </div>
        </div>
